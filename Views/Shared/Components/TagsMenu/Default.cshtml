@model List<Tag>

<div class="card mb-2">
    <div class="card-header">
        <h5 class="card-title mb-0">New Tags</h5>
    </div>
    <div class="card-body">
        @foreach(var tag in Model)
        {
            <div class="mb-3">
                <h6 class="mb-0">
                    <a href="/posts/details/1">
                        @tag.Text
                    </a>
                </h6>
            </div>
        }
    </div>
</div>